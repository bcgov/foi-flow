  <bpmn:process id="payment_submission" name="Payment Submission" isExecutable="true" camunda:versionTag="3">
    <bpmn:serviceTask id="Activity_1r0oka0" name="Get Token">
      <bpmn:extensionElements>
        <camunda:connector>
          <camunda:inputOutput>
            <camunda:inputParameter name="url">${keycloakURL}/auth/realms/${realm}/protocol/openid-connect/token</camunda:inputParameter>
            <camunda:inputParameter name="method">POST</camunda:inputParameter>
            <camunda:inputParameter name="headers">
              <camunda:map>
                <camunda:entry key="Content-Type">application/x-www-form-urlencoded</camunda:entry>
                <camunda:entry key="Authorization">Basic ${base64userpass}</camunda:entry>
                <camunda:entry key="Accept">application/json</camunda:entry>
              </camunda:map>
            </camunda:inputParameter>
            <camunda:inputParameter name="payload">grant_type=client_credentials</camunda:inputParameter>
            <camunda:outputParameter name="accessToken">${S(response).prop("access_token").stringValue()}</camunda:outputParameter>
          </camunda:inputOutput>
          <camunda:connectorId>http-connector</camunda:connectorId>
        </camunda:connector>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1o3gosy</bpmn:incoming>
      <bpmn:incoming>Flow_0w9yqn4</bpmn:incoming>
      <bpmn:outgoing>Flow_17bdgfc</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_17bdgfc" sourceRef="Activity_1r0oka0" targetRef="Activity_0gfn3o4" />
    <bpmn:serviceTask id="Activity_0gfn3o4" name="Invoke Request Details API">
      <bpmn:extensionElements>
        <camunda:connector>
          <camunda:inputOutput>
            <camunda:inputParameter name="url">${foiApiUrl}/api/foirequests/${requestid}/ministryrequest/${ministryid}/payonline</camunda:inputParameter>
            <camunda:inputParameter name="method">GET</camunda:inputParameter>
            <camunda:inputParameter name="headers">
              <camunda:map>
                <camunda:entry key="Content-Type">application/json</camunda:entry>
                <camunda:entry key="Authorization">Bearer ${accessToken}</camunda:entry>
              </camunda:map>
            </camunda:inputParameter>
            <camunda:outputParameter name="responseCode">
              <camunda:script scriptFormat="javascript">if (connector.getVariable("statusCode") != 200) {
	throw new org.camunda.bpm.engine.delegate.BpmnError("FOI-RAWREQUEST-API-ERR", response);
}
connector.getVariable("statusCode");</camunda:script>
            </camunda:outputParameter>
            <camunda:outputParameter name="axisRequestId">${S(response).prop("axisRequestId").stringValue()}</camunda:outputParameter>
            <camunda:outputParameter name="fee">${S(response).prop("balanceDue").numberValue()}</camunda:outputParameter>
            <camunda:outputParameter name="fileNumber">${S(response).prop("idNumber").stringValue()}</camunda:outputParameter>
            <camunda:outputParameter name="ministryRequestId">${ministryid}</camunda:outputParameter>
            <camunda:outputParameter name="foiRequestId">${requestid}</camunda:outputParameter>
            <camunda:outputParameter name="firstName">${S(response).prop("firstName").stringValue()}</camunda:outputParameter>
            <camunda:outputParameter name="middleName">${S(response).prop("middleName").stringValue()}</camunda:outputParameter>
            <camunda:outputParameter name="lastName">${S(response).prop("lastName").stringValue()}</camunda:outputParameter>
            <camunda:outputParameter name="assignedToFirstName">${S(response).prop("assignedToFirstName").stringValue()}</camunda:outputParameter>
            <camunda:outputParameter name="assignedToLastName">${S(response).prop("assignedToLastName").stringValue()}</camunda:outputParameter>
            <camunda:outputParameter name="assignedGroup">${S(response).prop("assignedGroup").stringValue()}</camunda:outputParameter>
            <camunda:outputParameter name="email">${S(response).prop("email").stringValue()}</camunda:outputParameter>
            <camunda:outputParameter name="apiUrl">${foiApiUrl}</camunda:outputParameter>
          </camunda:inputOutput>
          <camunda:connectorId>http-connector</camunda:connectorId>
        </camunda:connector>
        <camunda:executionListener event="end">
          <camunda:script scriptFormat="javascript">var current = execution.getVariable("retry");
execution.setVariable("retry", current+1); </camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_17bdgfc</bpmn:incoming>
      <bpmn:outgoing>Flow_0hjhi4p</bpmn:outgoing>
